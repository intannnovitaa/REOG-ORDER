<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reogorder">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/img_logo"
        android:label="Reog Order"
        android:roundIcon="@drawable/img_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ActivityDetail"
            android:exported="false" />
        <activity
            android:name=".customer.ActivityCheckout"
            android:exported="false" />
        <activity
            android:name=".admin.ActivityEditAdmin"
            android:exported="false" />
        <activity
            android:name=".admin.ActivityUtamaAdmin"
            android:exported="false" />
        <activity
            android:name=".customer.ActivityItem"
            android:exported="false" />
        <activity
            android:name=".ActivityAbout"
            android:exported="false" />
        <activity
            android:name=".ActivityLoading"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityWelcome"
            android:exported="false" />
        <activity
            android:name=".ActivityRegister"
            android:exported="false" />
        <activity
            android:name=".ActivityLogin"
            android:exported="false" />
        <activity
            android:name=".customer.ActivityUtama"
            android:exported="false" />
    </application>

</manifest>